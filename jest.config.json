{
  "testEnvironment": "node",
  "silent": false,
  "verbose": true,
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "transform": {
    "^.+\\.(t|j)sx?$": "@swc/jest"
  },
  "testPathIgnorePatterns": ["/node_modules/"],
  "testMatch": ["**/__tests__/**/*.[jt]s?(x)", "**/?(*.)+(spec|test).[tj]s?(x)"]
}
